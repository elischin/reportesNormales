	<parameter name="arrayIdalus" class="java.lang.String"/>
	<queryString>
		<![CDATA[select n.cct,n.nombreEscuela,
replace(substring(n.nombreEscuela,1,locate(' ',n.nombreEscuela)-1),'"','') as tipoesc,
n.cveinstitucion,"" as folio,a.tipocerti,
a.idalu,ag.idcct,ag.planestud,ag.semestre,
a.curp, a.nombre, a.apepat, a.apemat,concat(a.nombre,' ',a.apepat,' ',a.apemat) as nom_tot,
(select case when length(trim(especialidad))=0 then trim(licenciatura) else concat(licenciatura,' ',especialidad) end
        from siceen.catnormallic where cvecarrera = ag.cvecarrera and idcct = ag.idcct and genero = substring(a.curp,11,1)) as nomAcredit,
(select descarrera from catcarreras where cvecarrera = ag.cvecarrera) as desCarrera,
ag.planestud as plan_estud,pa.creditos,pa.curs_asig,upper(pa.tipoCreditos) as tipoCreditos,
(SELECT letra from cursosasignados where curs_asig = pa.curs_asig) as cursAsig_letra,
IFNULL((select sum(creditos) from alumnomaterias_certi ac,  plan_materias pm, materias m
        where ac.idalu=a.idalu AND ac.idcct=ag.idcct and ac.cvetipmat=pm.cvetipmat  AND ac.cvemat = pm.cvemat AND pm.cvecarrera=ag.cvecarrera AND pm.idmateria=m.idmateria AND ac.promedio>=6),'') AS creditos_aprob,
(select COUNT(*) from alumnomaterias_certi WHERE idalu=a.idalu AND promedio>=6) AS curs_aprob,
IFNULL((SELECT letra from cursosasignados where curs_asig =(select count(*) from alumnomaterias_certi WHERE idalu=a.idalu AND promedio>=6)),'') as cursAprob_letra,
a.cicescinilib,(a.cicescinilib+1) as cicescfinlib,ag.cicescini,
(select promediogral from alumnogdo_certi agc where agc.idalu = ag.idalu and agc.cvecarrera = ag.cvecarrera and semestre=8) as promediogral,ag.cvecarrera,
n.desmunicipio, a.tipocerti,
"" AS sellodigitalieepo, "" AS fechatimbradoieepo,"" AS fecha_texto,
"" AS firmante,
"" AS cargofirmante,
"" AS numcertfirmante,
(select count(*) from alumnomaterias_certi where idalu=a.idalu) as total_mat,
(select max(length(desmat)) from alumnomaterias_certi amc, plan_materias pm, materias m
where ag.semestre = amc.semestre and ag.idalu = amc.idalu and amc.semestre = pm.semestre and
ag.cvecarrera = pm.cvecarrera and amc.cvetipmat = pm.cvetipmat and amc.cvemat = pm.cvemat and pm.idmateria = m.idmateria) as longmat
from catnormal n, alumno_certi a, alumnogdo_certi ag, siceen.plan_carrera pa
where a.cicescinilib = 2020
$P!{arrayIdalus}
/*and ag.idcct= 14126*/
/*and a.idalu = 2803*/
and n.idcct = ag.idcct
and a.tipocerti = 'D'
and ag.cvecarrera = pa.cvecarrera
and ag.planestud = pa.plan_estud
and a.idalu = ag.idalu
group by n.cct,a.idalu
order by n.cct,a.idalu]]>
	</queryString>
